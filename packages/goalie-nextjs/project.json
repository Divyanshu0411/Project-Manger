{
	"name": "goalie-nextjs",
	"$schema": "../../node_modules/nx/schemas/project-schema.json",
	"sourceRoot": "packages/goalie-nextjs/src",
	"projectType": "library",
	"tags": [],
	"targets": {
		"lint": {
			"executor": "@nx/linter:eslint",
			"outputs": ["{options.outputFile}"],
			"options": {
				"lintFilePatterns": ["packages/goalie-nextjs/**/*.{ts,tsx,js,jsx}"]
			}
		},
		"test": {
			"executor": "@nx/jest:jest",
			"outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
			"options": {
				"jestConfig": "packages/goalie-nextjs/jest.config.ts",
				"passWithNoTests": true
			},
			"configurations": {
				"ci": {
					"ci": true,
					"codeCoverage": true
				}
			}
		}
	}
}
